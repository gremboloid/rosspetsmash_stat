<div class="report_table_content">
	
	<h3>{{ report_type }}: {{ report_type_name }}</h3>
	{% if data_count == 1 %}	
	<h4>{{ units }}: {{ data_units[0] }} {{ additional_data }}</h4>
	{% endif %}	
	<table class="outreport">
		<tbody>
			<tr class="head {% if subheader %} subhead-present{% endif %}">
			{% for col in other_columns %}
				<td>{{ col }}</td>
			{% endfor %}
			{% for col in head_columns %}	
				<td{{ colspan }}>{{ col }}</td>
			{% endfor %}				
			</tr>
			{% if subheader %}
			<tr class="subhead">
			{% for col in header2_columns %}	
				<td>{{ col|raw }}</td>
			{% endfor %}
			</tr>
			{% endif %}
			{{ table_data|raw }}
			</tbody>
	</table>
</div>